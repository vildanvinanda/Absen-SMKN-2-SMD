<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="10dp"
    app:cardElevation="8dp"
    android:layout_margin="10dp"

    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="0dp"

            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#F1F1F1"
                >
                <View
                    android:layout_width="10dp"
                    android:layout_height="match_parent"
                    android:background="@color/ijoMuda"
                    android:id="@+id/pembatas"
                    />

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_toEndOf="@+id/pembatas"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginHorizontal="20dp"
                    android:id="@+id/tamplate"
                    >
                    <TextView
                        android:id="@+id/txtjdl"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Basket"
                        android:textColor="@color/black"
                        android:textSize="12dp"
                        android:fontFamily="@font/poppinssemibold"
                        android:ellipsize="end"
                        android:singleLine="true"
                        />

                    <TextView
                        android:layout_below="@+id/txtjdl"
                        android:id="@+id/txtjam"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="15:05"
                        android:textColor="@color/black"
                        android:textSize="12dp"
                        android:fontFamily="@font/poppinsregular"
                        android:ellipsize="end"
                        android:singleLine="true"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginHorizontal="20dp"
                    android:layout_alignParentEnd="true"
                    >
                    <TextView
                        android:id="@+id/txt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Tempat"
                        android:textColor="@color/black"
                        android:textSize="12dp"
                        android:fontFamily="@font/poppinssemibold"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:layout_alignParentEnd="true"
                        />

                    <TextView
                        android:layout_below="@+id/txt"
                        android:layout_alignParentEnd="true"
                        android:id="@+id/txttempat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sumedang"
                        android:textColor="@color/black"
                        android:textSize="12dp"
                        android:fontFamily="@font/poppinsregular"
                        android:ellipsize="end"
                        android:singleLine="true"
                        />
                </RelativeLayout>

                <TextView
                    android:layout_alignParentEnd="true"
                    android:layout_below="@+id/tamplate"
                    android:id="@+id/txttgl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="04-04-2022"
                    android:textColor="@color/black"
                    android:textSize="12dp"
                    android:fontFamily="@font/poppinsregular"
                    android:layout_marginTop="10dp"
                    android:layout_marginHorizontal="10dp"
                    />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </RelativeLayout>


</androidx.cardview.widget.CardView>